<Window x:Class="FTeamLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FTeamLauncher"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Account" HorizontalAlignment="center"></TextBlock>
            <TextBox Width="200" Height="70"></TextBox>
            <Button Click="Button_CLick" Content="Test" Width="70"></Button>
        </StackPanel>
    </Grid>
</Window>
